<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="_assets_primary_variables" name="verdigado scss variables" inherit_id="web._assets_primary_variables" priority="10">
        <xpath expr="//link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/verdigado_template_default/static/src/scss/primary_variables.scss"/>
        </xpath>
    </template>

    <template id="assets_common" name="web_enterprise assets common" inherit_id="web_enterprise.assets_common">
        <xpath expr="//link[@href='/web_enterprise/static/src/scss/ui.scss']" position="replace">
                <link rel="stylesheet" type="text/scss" href="/verdigado_template_default/static/src/scss/ui.scss"/>
        </xpath>
    </template>

    <template id="assets_frontend" name="verdigado style" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/verdigado_template_default/static/src/scss/verdigado_style.scss"/>
        </xpath>
    </template>
</odoo>
